{
    "api_url": {
        "description": "SiliconFlow API 的 URL 地址",
        "type": "string",
        "hint": "默认为官方地址",
        "default": "https://api.siliconflow.cn"
    },
    "api_keys": {
        "description": "SiliconFlow API 密钥列表",
        "type": "list",
        "hint": "可以填写多个，插件会自动轮换使用。使用 #视频增加key 等命令进行管理。",
        "items": { "type": "string", "description": "API 密钥" },
        "default": []
    },
    "default_model": {
        "description": "默认使用的视频生成模型",
        "type": "string",
        "hint": "例如：Wan-AI/Wan2.2-I2V-A14B",
        "default": "Wan-AI/Wan2.2-I2V-A14B"
    },
    "use_proxy": {
        "description": "是否启用网络代理",
        "type": "bool",
        "default": false
    },
    "proxy_url": {
        "description": "网络代理地址",
        "type": "string",
        "hint": "格式为 http://user:pass@host:port",
        "default": ""
    },
    "enable_user_limit": {
        "description": "【次数限制】是否启用【用户】次数限制",
        "type": "bool",
        "default": true
    },
    "enable_group_limit": {
        "description": "【次数限制】是否启用【群组】次数限制",
        "type": "bool",
        "default": false
    },
    "polling_interval": {
        "description": "【高级】任务状态轮询间隔（秒）",
        "type": "int",
        "default": 5
    },
    "polling_timeout": {
        "description": "【高级】任务超时时间（秒）",
        "type": "int",
        "default": 300
    },
    "default_fps": {
        "description": "【高级】计算视频帧数时使用的默认帧率 (FPS)",
        "type": "int",
        "hint": "注意：提高此值会显著增加生成视频所需的总帧数，请确保所选模型支持。大多数模型原生FPS较低 (如8-16)。",
        "default": 8
    },
    "user_whitelist": {
        "description": "【权限】用户白名单",
        "type": "list",
        "items": { "type": "string", "description": "用户QQ号" },
        "default": []
    },
    "user_blacklist": {
        "description": "【权限】用户黑名单",
        "type": "list",
        "items": { "type": "string", "description": "用户QQ号" },
        "default": []
    },
    "group_whitelist": {
        "description": "【权限】群聊白名单",
        "type": "list",
        "items": { "type": "string", "description": "群号" },
        "default": []
    }

}
